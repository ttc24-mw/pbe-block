{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "ES2021",
    "lib": [
      "dom",
      "dom.iterable",
      "ES2021"
    ],
    "importHelpers": true,
    "noEmitHelpers": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "verbatimModuleSyntax": false,
    "skipLibCheck": true,
    "allowJs": true,
    "baseUrl": "src",
    "outDir": "./dist/",
    "typeRoots": [
      "./node_modules/@types",
      "./module/Shared/assets/shared/typings"
    ],
    "paths": {
      "seo/*": ["../js-frontend/storybook-seo/*"],
      "assets/*": ["assets/*"],
      "config/*": ["config/*"],
      "tslib": ["node_modules/tslib/tslib.d.ts"]
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
  ]
}